        {currentMovie.awards.map((award)=>(\n                    <Award title={award.title} description={award.description}/>\n                  ))}\n              </StyledAwards>\n              <StyledSecondImg>\n                  <img src={currentMovie.secondaryImg} alt=\"Movie2\"/>\n              </StyledSecondImg>\n            </StyledDetails>\n         )}\n        </>\n    )\n}\n\nconst StyledDetails = styled.div`\n min-height:90vh;\n /* padding-bottom:10rem; */\n`\nconst StyledHeadline = styled.div`\n display:flex;\n justify-content:center;\n align-items:center;\n padding:5rem 0;\n color:white;\n font-size:2rem;\n font-weight:lighter;\n`\nconst StyledMainImg = styled.div`\n img{\n     width:100%;\n     height:70vh;\n     object-fit:cover;\n }\n`\nconst StyledSecondImg = styled.div`\nimg{\n    width:100%;\n}\n`\nconst StyledAwards = styled.div`\n min-height:80vh;\n display:flex;\n justify-content:space-around;\n align-items:center;\n margin:5rem 10rem;\n`\nconst StyledAward = styled.div`\n padding:5rem;\n h3{\n     font-size:2rem;\n }\n .line{\n     height:0.3rem;\n     background-color:#62da97;\n     width:100%;\n     margin:2rem 0;\n }\n p{\n     padding:2rem 0;\n }\n`\n// We are declaring a other component\nconst Award = ({title,description})=>{\n    return(\n        <StyledAward>\n          <h3>{title}</h3>\n          <div className=\"line\"></div>\n          <p>{description}</p>\n        </StyledAward>\n    )\n}\nexport default MovieDetails\n","/home/som/Documents/react-course/ch-3/capture/src/movieState.js",[],{"ruleId":"63","replacedBy":"64"},{"ruleId":"65","replacedBy":"66"},{"ruleId":"67","severity":1,"message":"68","line":8,"column":19,"nodeType":"69","messageId":"70","endLine":8,"endColumn":28},"no-native-reassign",["71"],"no-negated-in-lhs",["72"],"no-unused-vars","'setMovies' is assigned a value but never used.","Identifier","unusedVar","no-global-assign","no-unsafe-negation"]